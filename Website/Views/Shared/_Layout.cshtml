<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - Virto Commerce Marketplace</title>
	@Styles.Render("~/defaultCSS")
	@Scripts.Render("~/defaultJS")

</head>
<body>
	<div class="grid"></div>
	<div id="layout">
		<div class="layout-canvas">
			@Html.Partial("_SwipePartial")
			@Html.Partial("_HeaderPartial")
			@RenderBody()
			@Html.Partial("_FooterPartial")
		</div>
	</div>
</body>
</html>
